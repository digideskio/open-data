<% semantic_form_for @dataset, :url => datasets_path do |form| %>
  <% form.inputs do -%>
    <%= form.input :title %>
    <%= form.input :description %>
    <%= form.input :start_date, :as => :string, :hint => 'mm/dd/YYYY' %>
    <%= form.input :end_date, :as => :string, :hint => 'mm/dd/YYYY' %>
    <%= form.input :is_featured, :required => false %>
  <% end -%>
  
  <% form.buttons do -%>
    <%= form.commit_button %>
  <% end -%>
<% end %>
